<div id="page-wrapper">
    <div class="container-fluid">
        <!-- Page Heading -->
        <div class="col-lg-12">
            <div class="col-lg-8">


            </div>
        </div>
        <!-- /.row -->
        <div class="row">
        @model IEnumerable<skool.Models.Student>

        @{
            ViewBag.Title = "Index";
        }

        <h2>Index</h2>

        <p>
            @Html.ActionLink("Create New", "Create")
        </p>
        @using (Html.BeginForm())
        {
            <p>
                Firstname: @Html.TextBox("SearchString")

                <input type="submit" value="Search" />
            </p>
        }
        <table class="table">
            <tr>
                <th>
                    Sort by:
                </th>
                <th>
                    @Html.ActionLink("First Name", "Index", new
{
    sortOrder =
        ViewBag.NameSortParm
})
                </th>

                <th>
                    @Html.ActionLink("Enrollment Date", "Index", new
{
    sortOrder =
        ViewBag.DateSortParm
})
                </th>
                <th></th>
            </tr>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.FirstName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.MiddleName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.LastName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EnrollmentDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Age)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Sex)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Status)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Section)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Classroom)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EmergencyContact)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Kebele)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Woreda)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SubCity)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.HouseNo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.username)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.password)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Confirmpassword)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.FirstName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.MiddleName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.LastName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.EnrollmentDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Age)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Sex)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Status)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Section)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Classroom)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.EmergencyContact)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Kebele)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Woreda)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.SubCity)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.HouseNo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.username)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.password)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Confirmpassword)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.StudentID }) |
                        @Html.ActionLink("Details", "Details", new { id = item.StudentID }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.StudentID })
                    </td>
                </tr>
            }

        </table>
        </div>
        <!-- /.row -->

    </div>
    <!-- /.container-fluid -->
</div>





