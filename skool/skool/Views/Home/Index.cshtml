        </div>
        <!-- /.row -->
        <div class="row">
            @model IEnumerable<skool.ViewModels.EnrollmentDateGroup>
            @{
                ViewBag.Title = "Student Statistics";
            }
            <h2>Student Statistics</h2>
            <table>
                <tr>
                    <th>
                        Enrollment Date
                    </th>
                    <th>
                        Students
                    </th>
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(ModelItem => item.EnrollmentDate)
                        </td>
                        <td>
                            @item.StudentCount
                        </td>
                    </tr>
                }
            </table>
        </div>
        <!-- /.row -->

    </div>
    <!-- /.container-fluid -->
</div>



